#include<stdio.h>
#include<stdlib.h>

int main() {
    // Write C code here
    int a,b;
    printf("Enter value of a and b\n");
    scanf("%d%d",&a,&b);
    
    if(a>b)
    {
        if(b!=0)
        {
           if(a%b==0)
          {
            printf("%d",0);
          }
          else
           printf("%d",1);
        }else
         printf("out of bound\n");
    }
    else{
        if(a!=0)
        {
            if(b%a==0)
           {
            printf("%d",0);
            }
            else{
             printf("%d",1);
            }
        }
        else{
            printf("Out of bound\n");
        }
    }
    return 0;
}
